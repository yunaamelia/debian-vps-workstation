[flake8]
max-line-length = 120
extend-ignore = E203, W503, E501, F841, E303
exclude = .git,__pycache__,docs/source/conf.py,old,build,dist,.venv,.mypy_cache
per-file-ignores =
    tests/*: E402
    configurator/modules/__init__.py: E402, F401
    configurator/utils/file.py: E402
    configurator/utils/network.py: E402
